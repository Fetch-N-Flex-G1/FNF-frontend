<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>servlets.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>signup</servlet-name>
        <servlet-class>servlets.SignUp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmergencyCon</servlet-name>
        <servlet-class>servlets.EmergencyCon</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Reminder</servlet-name>
        <servlet-class>servlets.Reminder</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Feedback</servlet-name>
        <servlet-class>servlets.Feedback</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>user_de.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>user_de</servlet-name>
        <servlet-class>servlets.UserDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PetDetails</servlet-name>
        <servlet-class>servlets.PetDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedbackServe</servlet-name>
        <servlet-class>servlets.FeedbackServe</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FeedbackForm</servlet-name>
        <servlet-class>servlets.FeedbackForm</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ContactUs</servlet-name>
        <servlet-class>servlets.ContactUs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserVerify</servlet-name>
        <servlet-class>servlets.UserVerify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>VerifyCode</servlet-name>
        <servlet-class>servlets.VerifyCode</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmergencyCon</servlet-name>
        <url-pattern>/EmergencyCon</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Reminder</servlet-name>
        <url-pattern>/Reminder</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>JspServlet</servlet-name>
        <servlet-class>org.apache.jasper.servlet.JspServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JspServlet</servlet-name>
        <url-pattern>*.jsp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Feedback</servlet-name>
        <url-pattern>/Feedback</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>user_de</servlet-name>
        <url-pattern>/user_de</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PetDetails</servlet-name>
        <url-pattern>/PetDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedbackServe</servlet-name>
        <url-pattern>/FeedbackServe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FeedbackForm</servlet-name>
        <url-pattern>/FeedbackForm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ContactUs</servlet-name>
        <url-pattern>/ContactUs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserVerify</servlet-name>
        <url-pattern>/UserVerify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>VerifyCode</servlet-name>
        <url-pattern>/VerifyCode</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
